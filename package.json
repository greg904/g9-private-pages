{
  "name": "g9-private-pages",
  "version": "0.1.0",
  "description": "",
  "main": "./dist/main.js",
  "scripts": {
    "build": "concurrently npm:build-ts npm:build-css",
    "build-ts": "tsc",
    "build-css": "postcss src/styles --dir static/styles",
    "watch": "concurrently npm:watch-ts npm:watch-css",
    "watch-ts": "tsc -w",
    "watch-css": "postcss src/styles --dir static/styles --watch",
    "serve": "node .",
    "serve-debug": "nodemon --inspect .",
    "debug": "npm run build && concurrently npm:watch npm:serve-debug",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "private": true,
  "devDependencies": {
    "@types/node": "^14.0.27",
    "@types/notp": "^2.0.0",
    "@types/qrcode": "^1.3.5",
    "concurrently": "^5.2.0",
    "cssnano": "^4.1.10",
    "nodemon": "^2.0.4",
    "postcss-cli": "^7.1.1",
    "postcss-cssnext": "^3.1.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@types/send": "^0.14.5",
    "base32-encode": "^1.1.1",
    "dotenv": "^8.2.0",
    "notp": "^2.0.3",
    "qrcode": "^1.4.4",
    "send": "^0.17.1"
  }
}
