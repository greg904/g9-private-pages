{
  "name": "g9-private-web",
  "version": "0.1.0",
  "description": "An HTTP server that serves static files behind a log-in form",
  "main": "./dist/main.js",
  "scripts": {
    "build": "node ./build-script --clean && concurrently tsc \"node ./build-script --html --css --js\" && copyfiles package*.json dist/",
    "start": "concurrently \"open-cli https://localhost:8080/\" \"cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only src/main.ts\" \"node ./build-script --watch --html --css --js\""
  },
  "private": true,
  "devDependencies": {
    "@types/mime-types": "^2.1.0",
    "@types/node": "^14.0.27",
    "@types/nunjucks": "^3.1.3",
    "concurrently": "^5.2.0",
    "copyfiles": "^2.3.0",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "google-closure-compiler": "^20200719.0.0",
    "html-minifier": "^4.0.0",
    "minimist": "^1.2.5",
    "open-cli": "^6.0.1",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "source-map-support": "^0.5.19",
    "ts-node-dev": "^1.0.0-pre.56",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "mime-types": "^2.1.27",
    "nunjucks": "^3.2.2"
  }
}
